<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="tb.domain">
	<class name="BrandService" table="brandservices">
		<id name="id">
			<generator class="identity" />
		</id>
		<many-to-one name="partner" column="partnerid" lazy="false" not-null="true"/>
		<property name="priority"></property>
		<property name="major"></property>
		<many-to-one name="brand" column="brandid" lazy="false" not-null="true"/>
	</class>
</hibernate-mapping>